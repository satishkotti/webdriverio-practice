                <xsl:attribute name="class">
                    <xsl:value-of select="$arrow_class"/>
                </xsl:attribute>
                <xsl:text disable-output-escaping="yes"></xsl:text>
            </xsl:element>
            <xsl:if test="$is_next = 'false'">
