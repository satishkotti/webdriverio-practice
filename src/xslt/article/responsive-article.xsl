        function onImageSlideShowNavClick(event) {
            var targetEle = $(event.target).closest(".nav-arrow"),
                isNext = targetEle.hasClass("icon-arrow-right"),
                allNavigables = $(".article-content .navigable"),
                totalCnt = allNavigables.length,
                currIndex = 0,
