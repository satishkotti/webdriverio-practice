                    </xsl:with-param>
                    <xsl:with-param name="upper_page">
                        <xsl:value-of select="$upper_page"/>
                    </xsl:with-param>
                </xsl:call-template>

                <xsl:choose>
                    <xsl:when test="$current_page >= $last_page">
                        <xsl:element name="span">
                            <xsl:attribute name="class">
                                <xsl:text>icon-arrow-right disabled</xsl:text>
                            </xsl:attribute>
                        </xsl:element>
